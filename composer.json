{
    "name": "zigzagdev/refilter",
    "description": "A minimal array filter and reindex helper for PHP.",
    "keywords": [
        "array",
        "helper",
        "filter",
        "reindex",
        "laravel",
        "zigzagdev"
    ],
    "homepage": "https://github.com/zigzagdev/refilter",
    "license": "MIT",
    "authors": [
        {
            "name": "zigzagdev",
            "email": "msts.oo0131@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/contracts": "^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.0 || ^8.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/extension-installer": "^1.3 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Zigzagdev\\Refilter\\": "src/"
        },
        "files": [
            "src/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zigzagdev\\App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zigzagdev\\Refilter\\RefilterServiceProvider"
            ],
            "aliases": {
                "Refilter": "Zigzagdev\\Refilter\\Facades\\Refilter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
